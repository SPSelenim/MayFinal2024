public class POMHomeScr {

	public WebDriver driver=null;
	
	public WebElement getFirstName()
	{
		WebElement FN=
	try
	{
		driver.findElement(By.id(FN));
	}
	catch(Exception e)
	{
		System.out.println(e.getMessage());
	}
	}
}
